<script>
// Sistema de carrinho completo
let cart = [];
let cartCount = 0;
let selectedBrands = {};

// Função para selecionar marca
function selectBrand(productId, brandName, price) {
// Remover seleção anterior do mesmo produto
const productCard = event.target.closest('.product-card');
const previousSelected = productCard.querySelector('.brand-item.selected');
if (previousSelected) {
previousSelected.classList.remove('selected');
}

// Selecionar nova marca
event.target.closest('.brand-item').classList.add('selected');

// Armazenar seleção
selectedBrands[productId] = {
name: brandName,
price: price
};

// Mostrar área de seleção
const selectedArea = document.getElementById(`selected-${productId}`);
const brandNameSpan = document.getElementById(`brand-name-${productId}`);
const brandPriceSpan = document.getElementById(`brand-price-${productId}`);

selectedArea.style.display = 'block';
brandNameSpan.textContent = brandName;
brandPriceSpan.textContent = `R$ ${price.toFixed(2)}`;
}

// Função para adicionar marca selecionada ao carrinho
function addSelectedToCart(productId) {
const selected = selectedBrands[productId];
if (!selected) {
alert('Por favor, selecione uma marca primeiro!');
return;
}

const productName = `${selected.name}`;
const price = selected.price;
const existingItem = cart.find(item => item.name === productName);

if (existingItem) {
existingItem.quantity += 1;
} else {
cart.push({
name: productName,
price: price,
quantity: 1
});
}

updateCartCount();
updateCartDisplay();

// Animação de feedback
const button = event.target;
const originalText = button.textContent;
button.textContent = '✓ Adicionado!';
button.style.background = '#28a745';

setTimeout(() => {
button.textContent = originalText;
button.style.background = '#28a745';
}, 1500);

showNotification(`${productName} adicionado ao carrinho!`);
}

// Produtos com preços
const productPrices = {
'Arroz': 4.90,
'Macarrão': 3.80,
'Açúcar': 3.50,
'Feijão': 7.20,
'Leite em Pó': 18.90,
'Achocolatados': 8.90,
'Leite em Caixa': 4.20,
'Manteiga': 9.80,
'Chicletes': 2.50,
